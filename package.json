{
  "name": "ripple-account-family",
  "version": "1.3.2",
  "description": "ripple account family",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "example",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/you21979/node-ripple-account-family.git"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "ripple-account-family",
    "ripple",
    "wallet"
  ],
  "dependencies": {
    "bn.js": "5.0.0",
    "create-hash": "1.2.0",
    "elliptic": "6.5.3",
    "randombytes": "2.1.0",
    "ripple-bs58check": "2.0.2"
  },
  "devDependencies": {
    "@types/bn.js": "^4.11.5",
    "@types/create-hash": "^1.2.0",
    "@types/elliptic": "^6.4.10",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.7.5",
    "coveralls": "^3.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "ripple-address-codec": "3.0.4",
    "ripple-keypairs": "1.0.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "./node_modules/.bin/tsc -p ./",
    "format": "npm run prettier -- --write",
    "prettier": "prettier 'src/**/*.ts'",
    "coverage": "npm run unit -- --coverage",
    "unit": "jest --config=jest.json --runInBand",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test": "npm run coverage",
    "tsc": "tsc -p tsconfig.json"
  },
  "author": "Yuki Akiyama",
